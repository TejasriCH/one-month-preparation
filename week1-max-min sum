class Result {
    public static void miniMaxSum(List<Integer> arr) {
        Collections.sort(arr);
      long sum1=0;
      long sum2=0;
      for(int i=1;i<arr.size();i++){
          sum2+=arr.get(i);
      }
      for(int i=0;i<arr.size()-1;i++){
         sum1=sum1+arr.get(i);
      }
      
      System.out.println(sum1+" "+sum2);
    }

}
